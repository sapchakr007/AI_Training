import google.generativeai as genai
import os

# configuration
genai.configure(api_key=os.getenv("AIzaSyC2OUGzujkOhkBMZmbXfMjNGw3SGx2fVpM"))
#genai.configure(api_key="AIzaSyC2OUGzujkOhkBMZmbXfMjNGw3SGx2fVpM")

generation_config={"temperature":0.9, "top_p": 1, "top_k":1, "max_output_tokens":2048}

#initialize the model
model=genai.GenerativeModel("gemini-2.0-flash", generation_config=generation_config)

#Generate content

response=model.generate_content(["Who is Salman khan"])
print(response.txt)