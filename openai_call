response = client.chat.completions.create(
   model="sonar-reasoning-pro",
    messages=[{"role": "user", "content": "write a simple regression algorithm"}],
    
    response_format={
        "type": "regex",
        "regex": { "regex": ".*" }}
)
print(response.choices[0].message.content)